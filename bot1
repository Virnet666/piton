comand=input("Впишите команду")
pers={"Name":"Glem","Age":"Never grow old","Spel":"ROOOOOOOOOOOOOKS","hobby":"cooking","Job":"instrument teacher","like music":"ROOOOOOOOOOOOCK"}
while True:
    if comand == "New":
        namenew=input("Имя нового своиства персонажа")
        incluednew=input("Что представляет это свойство")
        pers[namenew]=incluednew
        comand=input("Введите команду")
    if comand == "print":
        print(pers)
        comand=input("Введите команду")
comand=input("Введите команду")
